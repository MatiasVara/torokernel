<domain type="kvm">
  <name>ToroHelloWorldMicroservice</name>
  <uuid>ba67c0b4-bfd9-47b9-b8d0-fcaee3d25d2e</uuid>
  <memory>524288</memory>
  <currentMemory>524288</currentMemory>
  <vcpu>1</vcpu>
  <os>
    <type arch="x86_64">hvm</type>
    <boot dev="hd"/>
  </os>
  <features>
    <acpi/>
    <apic/>
  </features>
  <cpu mode="custom" match="exact">
    <model>Westmere-IBRS</model>
  </cpu>
  <clock offset="utc">
    <timer name="rtc" tickpolicy="catchup"/>
    <timer name="pit" tickpolicy="delay"/>
    <timer name="hpet" present="no"/>
  </clock>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>restart</on_reboot>
  <on_crash>restart</on_crash>
  <pm>
    <suspend-to-mem enabled="no"/>
    <suspend-to-disk enabled="no"/>
  </pm>
  <devices>
    <emulator>/usr/bin/kvm-spice</emulator>
    <disk type="file" device="disk">
      <driver name="qemu" type="raw"/>
      <source file="/root/torokernel/torokernel-fixfornetworking/examples/ToroHelloWorldMicroservice/ToroHelloWorldMicroservice.img"/>
      <target dev="hda" bus="ide"/>
    </disk>
    <interface type="bridge">
      <source bridge="toro-bridge"/>
      <mac address="52:54:00:3e:33:2b"/>
      <model type="virtio-net-pci"/>
    </interface>
    <graphics type="vnc" port="-1"/>
    <console type="pty"/>
  </devices>
</domain>
